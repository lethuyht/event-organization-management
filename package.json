{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "webpack --watch --progress",
    "build": "rm -rf dist && yarn webpack",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node --enable-source-maps .webpack/main.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "cross-env APP_ENV=test jest --detectOpenHandles --testTimeout=30000 --config ./test/jest-e2e.json",
    "test": "yarn test:integration --forceExit",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -d src/db/migrations -n",
    "migration:run": "ts-node -r tsconfig-paths/register src/db/runMigrations.ts apply",
    "migration:revert": "ts-node -r tsconfig-paths/register src/db/runMigrations.ts revert",
    "migration:seed": "ts-node src/db/seeds/scripts/initDataCountries.ts",
    "api:gen": "nest g module $NAME $ROOT && nest g controller $NAME $ROOT && nest g service $NAME $ROOT",
    "prepare": "husky install",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "doc": "npx compodoc --port 11045 -p tsconfig.json -s",
    "doc:serve": "yarn doc && npx compodoc -s"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/common": "^9.0.1",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^9.0.1",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.1",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/typeorm": "^7.1.0",
    "apollo-datasource-rest": "^0.14.0",
    "apollo-server-core": "^3.10.1",
    "apollo-server-express": "^3.9.0",
    "apollo-server-fastify": "^2.9.15",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.1",
    "dayjs": "^1.11.7",
    "dotenv": "^8.2.0",
    "exceljs": "^4.2.1",
    "express": "^4.17.1",
    "geojson": "^0.5.0",
    "global": "^4.4.0",
    "google-auth-library": "^8.5.2",
    "graphql": "^16.5.0",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-tools": "^7.0.5",
    "graphql-type-json": "^0.3.2",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-range": "^4.0.2",
    "nanoid": "^3.1.22",
    "nest-raven": "^7.2.0",
    "nestjs-i18n": "^8.0.11",
    "nestjs-typeorm-paginate": "^2.5.1",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "ramda": "^0.27.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^7.5.5",
    "slug": "^4.0.3",
    "stripe": "^8.156.0",
    "typeorm": "^0.2.25",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.3.1",
    "@types/express": "^4.16.0",
    "@types/faker": "^5.5.1",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^10.7.1",
    "@types/nodemailer": "^6.4.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/redis": "^2.8.30",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.5",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "aws-sdk": "^2.642.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "faker": "^5.5.3",
    "fork-ts-checker-webpack-plugin": "7.2.12",
    "fs-extra": "^7.0.1",
    "husky": "^8.0.1",
    "jest": "^26.1.0",
    "jest-json-schema": "^5.0.0",
    "nodemon-webpack-plugin": "4.7.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "transform-class-properties": "^1.0.0-beta",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm-factory": "^0.0.14",
    "typescript": "^4.7.4",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
